results = await client.conn.fetch("""
    SELECT 
        m.*,
        c.title AS conversation_title,
        c.type AS conversation_type
    FROM messages m
    LEFT JOIN conversations c ON m.conversation_id = c.conversation_id
    ORDER BY m."createdAt" DESC
""")


